      <div class="ArticlePage-aside col-xl-3 col-lg-4 col-5 Xd-inline-block d-block float-right sidebar hidden-print"> 
       <aside class="ArticlePage-aside">
        <div id="article-newsletter-container"></div> 
        <div id="googleAda6884672-b73e-4335-b51b-948f6356b70c" class="GoogleDfpAd" data-slot-name="/4026082/washington_examiner/sidebar_1" data-slot-sizes="[[300, 600], [300, 250], [300, 1050]]" data-slot-adsizemap="[[[1024, 0], [300, 1050], [300, 600], [300, 250]], [[768, 0], [300, 600], [300, 250]]]" data-slot-position="2"> 
        </div> 
        <div id="wex_poll_container"></div> 
        <div id="googleAd9f963aa0-5a45-4b55-8c4a-c0084b996aa4" class="GoogleDfpAd" data-slot-name="/4026082/washington_examiner/sidebar_2" data-slot-sizes="[[300, 600], [300, 250]]" data-slot-adsizemap="[[[768, 0], [300, 600], [300, 250]]]" data-slot-position="3"> 
        </div> 
       </aside> 
