    /* Global Async */
    function init() {
        // ASYNC
    //    console.log('async');
    //var wotr_new_v1 = 0;

        var js_version = "1.0.3";
        var element2 = document.createElement("script");
        element2.src = "https://warontherocks.com/wp-content/themes/warontherocks/assets/js/script.js?v=" + js_version;
        document.body.appendChild(element2);

        var element3 = document.createElement("script");
        element3.src = "https://warontherocks.com/wp-content/themes/warontherocks/assets/js/ga_clicks.js?v=" + js_version;
        document.body.appendChild(element3);

        flint_code();
    //    mobile_popup();
    }
    if (window.addEventListener) window.addEventListener("load", init, false);
    else if (window.attachEvent) window.attachEvent("onload", init);
    else window.onload = init;
    
