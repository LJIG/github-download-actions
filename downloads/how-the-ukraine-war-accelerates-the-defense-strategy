        function flint_code() {
            $( ".subscribe button" ).on('click touch', function () {
                $( ".subscribe .hidden-sighup" ).addClass( "active" );
            });
            $( ".expand-form" ).on('click touch', function () {
                $( ".searchbar input" ).toggleClass( "active" );
                $( ".searchbar button" ).toggleClass( "active" );
                $(this).toggleClass('hidden');
            });
            $( ".hamburger" ).on('click touch', function () {
                $( ".new_menu" ).toggleClass( "active" );
                $( ".nav-opener" ).toggleClass( "active" );
            });
            $( ".got-it" ).on('click touch', function () {
                $( ".popup" ).toggleClass( "hidden" );
                Cookies.set('wotr_new_v1', '1', { expires:180 });
            });
            $( ".podcasts-active" ).on('click touch', function () {
                $(".wotr-active").removeClass('selected');
                $(".tnsr-active").removeClass('selected');
                $(this).addClass('selected');
                $( ".podcasts" ).removeClass( "hidden" );
                $( ".podcasts" ).addClass( "active" );
                $( ".latest-posts" ).removeClass( "active" );
                $( ".latest-posts" ).addClass( "hidden" );
                $( ".tnsr-posts" ).removeClass( "active" );
                $( ".tnsr-posts" ).addClass( "hidden" );
            });
            $( ".wotr-active" ).on('click touch', function (){
                $(".podcasts-active").removeClass('selected');
                $(".tnsr-active").removeClass('selected');
                $(this).addClass('selected');
                $( ".latest-posts" ).removeClass( "hidden" );
                $( ".latest-posts" ).addClass( "active" );
                $( ".podcasts" ).removeClass( "active" );
                $( ".podcasts" ).addClass( "hidden" );
                $( ".tnsr-posts" ).removeClass( "active" );
                $( ".tnsr-posts" ).addClass( "hidden" );
            });
            $( ".tnsr-active" ).on('click touch', function () {
                $(".podcasts-active").removeClass('selected');
                $(".wotr-active").removeClass('selected');
                $(this).addClass('selected');
                $( ".tnsr-posts" ).removeClass( "hidden" );
                $( ".tnsr-posts" ).addClass( "active" );
                $( ".latest-posts" ).removeClass( "active" );
                $( ".latest-posts" ).addClass( "hidden" );
                $( ".podcasts" ).removeClass( "active" );
                $( ".podcasts" ).addClass( "hidden" );
            });
        }
    function mobile_popup() {
        //Cookies.set('wotr_new_v1', '1', { expires:180 });
        var wotr_new_v1 = Cookies.get('wotr_new_v1');
        //console.log(wotr_new_v1);
        if (wotr_new_v1 != '1') {
            $( ".popup" ).toggleClass( "hidden" );
        }
    }

